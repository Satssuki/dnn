<?xml version="1.0"?>
<opencv_storage>
<data>
	<training_filename>data/addition/addition-training.xml.gz</training_filename>
	<response_filename>data/addition/addition-response.xml.gz</response_filename>
	<testing_filename>data/addition/addition-testing.xml.gz</testing_filename>
	<expected_filename>data/addition/addition-expected.xml.gz</expected_filename>
</data>
<network>
	<model_filename>data/addition/rnn_model.yml</model_filename>
	<weights_filename>data/addition/rnn_weights.xml</weights_filename>
	<lr_init>0.75</lr_init>
	<decay_type>invsqrt</decay_type>
	<maxiter>50000</maxiter>
  <n_epochs>20</n_epochs>
	<batch_size>2</batch_size>
	<validate_ratio>0.1</validate_ratio>
</network>
</opencv_storage>
