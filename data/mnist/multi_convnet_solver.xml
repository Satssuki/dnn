<?xml version="1.0"?>
<opencv_storage>
<data>
	<training_filename>data/mnist/multi_training.xml.gz</training_filename>
	<response_filename>data/mnist/multi_response.xml.gz</response_filename>
	<!-- <testing_filename>data/mnist/multi_testing.xml.gz</testing_filename> -->
	<testing_filename>data/heartrate/out3.xml.gz</testing_filename>
	<predicted_filename>data/heartrate/out3_predicted.xml.gz</predicted_filename>
	<expected_filename>data/mnist/multi_expected.xml.gz</expected_filename>
</data>
<network>
	<model_filename>data/mnist/multi_convnet_model.yml</model_filename>
	<weights_filename>data/mnist/multi_convnet_weights.xml</weights_filename>
	<lr_init>0.05</lr_init>
	<decay_type>invsqrt</decay_type>
	<maxiter>1000</maxiter>
	<batch_size>1</batch_size>
	<validate_ratio>0.1</validate_ratio>
</network>
</opencv_storage>
