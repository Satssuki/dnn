<?xml version="1.0"?>
<opencv_storage>
<data>
	<training_filename>data/mnist/train-images-idx3-ubyte.xml.gz</training_filename>
	<response_filename>data/mnist/train-labels-idx1-ubyte.xml.gz</response_filename>
	<testing_filename>data/mnist/t10k-images-idx3-ubyte.xml.gz</testing_filename>
	<expected_filename>data/mnist/t10k-labels-idx1-ubyte.xml.gz</expected_filename>
</data>
<network>
	<model_filename>data/mnist/lenet_model.xml</model_filename>
	<weights_filename>data/mnist/lenet_weights.xml</weights_filename>
	<lr_init>0.05</lr_init>
	<decay_type>invsqrt</decay_type>
	<maxiter>2000</maxiter>
	<batch_size>2</batch_size>
	<validate_ratio>0.1</validate_ratio>
</network>
</opencv_storage>
