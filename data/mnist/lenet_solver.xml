<?xml version="1.0"?>
<opencv_storage>
<data>
	<training_filename>data/mnist/lenet_training.xml.gz</training_filename>
	<response_filename>data/mnist/lenet_response.xml.gz</response_filename>
	<testing_filename> data/mnist/lenet_testing.xml.gz</testing_filename>
	<expected_filename>data/mnist/lenet_expected.xml.gz</expected_filename>
</data>
<network>
	<model_filename>data/mnist/lenet_model.yml</model_filename>
	<weights_filename>data/mnist/lenet_weights.xml</weights_filename>
	<lr_init>0.25</lr_init>
	<decay_type>invsqrt</decay_type>
	<maxiter>5000</maxiter>
	<batch_size>20</batch_size>
	<validate_ratio>0.1</validate_ratio>
</network>
</opencv_storage>
